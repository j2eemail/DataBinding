<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lxs.databinding">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".view.RemoveFindViewByIdActivity"></activity>
        <activity android:name=".view.UIBindingActivity"></activity>
        <activity android:name=".view.EventBindingActivity"></activity>
        <activity android:name=".view.IncludeActivity"></activity>
        <activity android:name=".view.ObservableActivity"></activity>
        <activity android:name=".view.ImageViewActivity"></activity>
        <activity android:name=".view.RecyclerViewActivity"></activity>

    </application>

</manifest>